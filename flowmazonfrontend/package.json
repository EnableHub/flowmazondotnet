{
  "name": "flowmazonfrontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prettier --check . && next build",
    "build:local": "prettier --write . && next build",
    "format": "prettier --write .",
    "start": "next start",
    "lint": "next lint",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "chromatic": "# do nothing as we don't want Chromatic project token in here",
    "test-storybook": "test-storybook",
    "test-storybook:test": "echo Starting Storybook tests... && (test-storybook --url http://127.0.0.1:6010 --watch --coverage || true) && echo Completed Storybook tests...",
    "test-storybook:watch": "concurrently --names \"STORYBOOK,TEST\" --prefix-colors \"blue,magenta\" \"storybook dev --no-open -p 6010\" \"wait-on http://127.0.0.1:6010 && wait 30000 && npm run test-storybook:test\"",
    "test-storybook-a11y:test": "wait 3000 && echo Starting Storybook A11Y tests... && ((axe-storybook --storybook-url http://127.0.0.1:6010) || true) && echo Completed Storybook A11Y tests...",
    "test-storybook-a11y:watch": "wait-on http://127.0.0.1:6010 && wait 30000 && onchange -i -k src/**/* -- npm run test-storybook-a11y:test"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^1.6.0",
    "@prisma/client": "^5.12.1",
    "@tailwindcss/typography": "^0.5.12",
    "clsx": "^2.1.1",
    "daisyui": "^4.10.1",
    "formik": "^2.4.5",
    "next": "^14.2.15",
    "next-auth": "^4.24.7",
    "prisma": "^5.12.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "yup": "^1.4.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@chanzuckerberg/axe-storybook-testing": "^8.2.2",
    "@chromatic-com/storybook": "^1.6.1",
    "@storybook/addon-a11y": "^8.6.11",
    "@storybook/addon-coverage": "^1.0.5",
    "@storybook/addon-essentials": "^8.6.11",
    "@storybook/addon-interactions": "^8.6.11",
    "@storybook/addon-links": "^8.6.11",
    "@storybook/addon-onboarding": "^8.6.11",
    "@storybook/addon-storysource": "^8.6.11",
    "@storybook/blocks": "^8.6.11",
    "@storybook/experimental-addon-test": "^8.6.11",
    "@storybook/experimental-nextjs-vite": "^8.6.11",
    "@storybook/nextjs": "^8.6.11",
    "@storybook/react": "^8.6.11",
    "@storybook/test": "^8.6.11",
    "@storybook/test-runner": "^0.19.1",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^8.9.0",
    "@typescript-eslint/parser": "^8.9.0",
    "@vitest/browser": "^3.0.9",
    "@vitest/coverage-v8": "^3.0.9",
    "autoprefixer": "^10.0.1",
    "chromatic": "^11.27.0",
    "concurrently": "^9.1.2",
    "eslint": "^8.57.1",
    "eslint-config-next": "^14.2.15",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-storybook": "^0.8.0",
    "eslint-plugin-tailwindcss": "^3.17.5",
    "jest-axe": "^9.0.0",
    "onchange": "^7.1.0",
    "playwright": "^1.51.1",
    "postcss": "^8",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.8",
    "storybook": "^8.1.11",
    "tailwindcss": "^3.3.0",
    "typescript": "^5.6.3",
    "vitest": "^3.0.9",
    "wait": "^0.4.2",
    "wait-on": "^8.0.3"
  }
}
