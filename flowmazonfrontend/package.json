{
  "name": "flowmazonfrontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "prettier --check . && next build",
    "build:local": "prettier --write . && next build",
    "format": "prettier --write .",
    "start": "next start",
    "lint": "next lint",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "chromatic": "# do nothing as we don't want Chromatic project token in here",
    "test-storybook": "test-storybook",
    "test-storybook:test": "wait 3000 && echo Starting Storybook tests... && (test-storybook --coverage || true) && echo Completed Storybook tests...",
    "test-storybook:watch": "concurrently --names \"STORYBOOK,TEST\" --hide HTTP --prefix-colors \"blue,magenta\" \"storybook dev -p 6006\" \"wait-on http://127.0.0.1:6006 && wait 10000 && onchange -i -k src/**/* -- npm run test-storybook:test\"",
    "test-storybook-a11y:test": "wait 3000 && echo Starting Storybook A11Y tests... && ((axe-storybook --storybook-url http://127.0.0.1:6006) || true) && echo Completed Storybook A11Y tests...",
    "test-storybook-a11y:watch": "wait-on http://127.0.0.1:6006 && wait 10000 && onchange -i -k src/**/* -- npm run test-storybook-a11y:test"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^1.6.0",
    "@guidepup/guidepup": "^0.24.0",
    "@prisma/client": "^5.12.1",
    "@tailwindcss/typography": "^0.5.12",
    "clsx": "^2.1.1",
    "formik": "^2.4.5",
    "next": "^15.3.0",
    "next-auth": "^4.24.11",
    "prisma": "^5.12.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "yup": "^1.4.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@chanzuckerberg/axe-storybook-testing": "^8.2.2",
    "@chromatic-com/storybook": "^3.2.6",
    "@guidepup/virtual-screen-reader": "^0.32.0",
    "@storybook/addon-a11y": "^8.6.12",
    "@storybook/addon-coverage": "^1.0.5",
    "@storybook/addon-essentials": "^8.6.12",
    "@storybook/addon-interactions": "^8.6.12",
    "@storybook/addon-links": "^8.6.12",
    "@storybook/addon-onboarding": "^8.6.12",
    "@storybook/addon-storysource": "^8.6.12",
    "@storybook/blocks": "^8.6.12",
    "@storybook/experimental-addon-test": "^8.6.12",
    "@storybook/experimental-nextjs-vite": "^8.6.12",
    "@storybook/jest": "^0.2.2",
    "@storybook/nextjs": "^8.6.12",
    "@storybook/react": "^8.6.12",
    "@storybook/test": "^8.6.12",
    "@storybook/test-runner": "^0.22.0",
    "@tailwindcss/postcss": "^4.1.4",
    "@types/node": "^20",
    "@types/react": "19.1.2",
    "@types/react-dom": "19.1.2",
    "@typescript-eslint/eslint-plugin": "^8.32.0",
    "@typescript-eslint/parser": "^8.32.0",
    "@vitest/browser": "^3.0.9",
    "@vitest/coverage-v8": "^3.0.9",
    "chai": "^5.2.0",
    "chromatic": "^11.28.0",
    "concurrently": "^9.1.2",
    "daisyui": "^5.0.22",
    "eslint": "^9.24.0",
    "eslint-config-next": "^15.3.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-storybook": "^0.12.0",
    "jest-axe": "^9.0.0",
    "msw": "^2.8.2",
    "msw-storybook-addon": "^2.0.4",
    "onchange": "^7.1.0",
    "playwright": "^1.51.1",
    "postcss": "^8",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "storybook": "^8.6.12",
    "tailwindcss": "^4.1.4",
    "typescript": "^5.6.3",
    "vitest": "^3.0.9",
    "wait": "^0.4.2",
    "wait-on": "^8.0.3"
  },
  "overrides": {
    "@types/react": "19.1.2",
    "@types/react-dom": "19.1.2"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}
